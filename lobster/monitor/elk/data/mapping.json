{
    "settings": {
        "index": {
            "refresh_interval": "5s"
        }
    },
    "mappings": {
        "_default_": {
            "properties": {
                "timestamp": {
                    "type": "date"
                }
            },
            "dynamic_templates": [{
                "string_fields": {
                    "mapping": {
                        "index": "analyzed",
                        "omit_norms": true,
                        "fielddata": {
                            "format": "disabled"
                        },
                        "type": "string",
                        "fields": {
                            "raw": {
                                "index": "not_analyzed",
                                "ignore_above": 256,
                                "type": "string"
                            }
                        }
                    },
                    "match_mapping_type": "string",
                    "match": "*"
                }
            }],
            "_all": {
                "enabled": true,
                "omit_norms": true
            }
        }
    },
    "aliases": {}
}
